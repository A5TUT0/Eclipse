  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5C6370",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#C678DD"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#61AFEF"
      }
    },
    {
      "scope": ["variable", "support.variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#ABB2BF"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#E5C07B"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#56B6C2"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#F44747",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#D19A66",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#98C379"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#E06C75"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#D19A66"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#5C6370"
      }
    }
