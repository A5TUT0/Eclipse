  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#757575",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#76FF03"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#FFAB00"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#F50057"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#448AFF"
      }
    },
    {
      "scope": ["variable", "support.variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#FFD180"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#FFD180"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#FF1744"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#FFAB00"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#00E5FF"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#00E5FF"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#FF5252",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#000000",
        "background": "#FFC400",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#76FF03"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#FF1744"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#FFAB00"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#757575"
      }
    }
  ]
}
