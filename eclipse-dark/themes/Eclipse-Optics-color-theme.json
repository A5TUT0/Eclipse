  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#737373",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#8CD9B3"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#B3864D"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#B38CD9"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#668099"
      }
    },
    {
      "scope": ["variable", "support.variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#D9B38C"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#D9B38C"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#D98C8C"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#B3864D"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#8CB3D9"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#8CB3D9"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#D96666",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#000000",
        "background": "#B3864D",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#8CD9B3"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#D98C8C"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#B3864D"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#737373"
      }
    }
  ]
}
