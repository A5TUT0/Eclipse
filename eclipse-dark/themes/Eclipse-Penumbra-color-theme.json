  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6B728E",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#A8DADC"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#F4A261"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#B2B5E0"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#8DB5E0"
      }
    },
    {
      "scope": ["variable", "support.variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#F1FAEE"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#A8DADC"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#F4A261"
      }
    },
    {
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#E63946"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#F4A261"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#A8DADC"
      }
    },
    {
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#A8DADC"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#F1FAEE",
        "background": "#E63946",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "foreground": "#1D3557",
        "background": "#F4A261",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#A8DADC"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#E63946"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#F4A261"
      }
    },
    {
      "scope": ["meta.diff", "meta.diff.header"],
      "settings": {
        "foreground": "#6B728E"
      }
    }
  ]
}
